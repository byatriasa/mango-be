{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "swc ./src -d ./dist",
    "clean": "rm -rf ./dist",
    "dev": "nodemon --exec \"node -r @swc-node/register ./src/api.ts\"",
    "start": "NODE_ENV=production node --enable-source-maps dist/api.js",
    "lint": "tsc --noEmit && eslint --ignore-path .gitignore \"**/*.{ts,js}\"",
    "format": "prettier --ignore-path .gitignore --write .",
    "run-script": "yarn build && node -r @swc-node/register",
    "test": "vitest",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "axios": "^1.6.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "tiny-invariant": "^1.3.3",
    "ts-node": "^10.9.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^36.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "luxon": "^3.4.4",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "typescript": "^5.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
